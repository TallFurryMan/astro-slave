<INDIDriver>
    <newSwitchVector device="Filter Simulator" name="DEBUG">
        <oneSwitch name="ENABLE">
Off
        </oneSwitch>
        <oneSwitch name="DISABLE">
On
        </oneSwitch>
    </newSwitchVector>
    <newSwitchVector device="Filter Simulator" name="DEBUG_LEVEL">
        <oneSwitch name="DBG_ERROR">
On
        </oneSwitch>
        <oneSwitch name="DBG_WARNING">
On
        </oneSwitch>
        <oneSwitch name="DBG_SESSION">
On
        </oneSwitch>
        <oneSwitch name="DBG_DEBUG">
Off
        </oneSwitch>
    </newSwitchVector>
    <newSwitchVector device="Filter Simulator" name="LOGGING_LEVEL">
        <oneSwitch name="LOG_ERROR">
On
        </oneSwitch>
        <oneSwitch name="LOG_WARNING">
On
        </oneSwitch>
        <oneSwitch name="LOG_SESSION">
On
        </oneSwitch>
        <oneSwitch name="LOG_DEBUG">
Off
        </oneSwitch>
    </newSwitchVector>
    <newSwitchVector device="Filter Simulator" name="LOG_OUTPUT">
        <oneSwitch name="CLIENT_DEBUG">
On
        </oneSwitch>
        <oneSwitch name="FILE_DEBUG">
Off
        </oneSwitch>
    </newSwitchVector>
    <newNumberVector device="Filter Simulator" name="FILTER_SLOT">
        <oneNumber name="FILTER_SLOT_VALUE">
1
        </oneNumber>
    </newNumberVector>
    <newTextVector device="Filter Simulator" name="FILTER_NAME">
        <oneText name="FILTER_SLOT_NAME_1">
      Red
        </oneText>
        <oneText name="FILTER_SLOT_NAME_2">
      Green
        </oneText>
        <oneText name="FILTER_SLOT_NAME_3">
      Blue
        </oneText>
        <oneText name="FILTER_SLOT_NAME_4">
      H_Alpha
        </oneText>
        <oneText name="FILTER_SLOT_NAME_5">
      SII
        </oneText>
        <oneText name="FILTER_SLOT_NAME_6">
      OIII
        </oneText>
        <oneText name="FILTER_SLOT_NAME_7">
      LPR
        </oneText>
        <oneText name="FILTER_SLOT_NAME_8">
      Luminosity
        </oneText>
    </newTextVector>
    <newSwitchVector device="Filter Simulator" name="USEJOYSTICK">
        <oneSwitch name="ENABLE">
Off
        </oneSwitch>
        <oneSwitch name="DISABLE">
On
        </oneSwitch>
    </newSwitchVector>
    <newTextVector device="Filter Simulator" name="JOYSTICKSETTINGS">
        <oneText name="Change Filter">
JOYSTICK_1
        </oneText>
        <oneText name="Reset">
BUTTON_1
        </oneText>
    </newTextVector>
</INDIDriver>
